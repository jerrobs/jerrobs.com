{
  "extends": "astro/tsconfigs/strict", 
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@astro/*": [
        "./src/_astro/*"
      ],
      "@*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    },
    "jsx": "preserve"
  }
}